(def-instance sentence-5.4-german sentence
((part-of-expression Tractatus-original-german-version)
 (has-form written-german) 
 (has-number-reference 5.4) 
 (has-string-content "Hier zeigt es sich, dass es &#187;logische Gegenst&#228;nde&#171;, &#187;logische Konstante&#171; (im Sinne Freges und Russells) nicht gibt.")))

(def-instance sentence-5.41-german sentence
((part-of-expression Tractatus-original-german-version)
 (has-form written-german) 
